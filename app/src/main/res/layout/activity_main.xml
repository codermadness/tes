<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    android:backgroundTint="@color/colorPrimary"
    tools:context="com.atmospheric.tes.iseng.MainActivity">

    <FrameLayout
        android:id="@+id/frameLayout6"
        android:layout_width="191dp"
        android:layout_height="178dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="6dp"
        android:background="#006837"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/frameLayout5"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout2">
     

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout5"
        android:layout_width="191dp"
        android:layout_height="178dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="2dp"
        android:background="@color/colorAccent"
        android:backgroundTint="#fbb03b"
        app:layout_constraintEnd_toStartOf="@+id/frameLayout6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout4">


    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="191dp"
        android:layout_height="178dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="3dp"
        android:background="@color/colorAccent"
        android:backgroundTint="#009245"
        app:layout_constraintEnd_toStartOf="@+id/frameLayout2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout">


    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="191dp"
        android:layout_height="178dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="6dp"
        android:background="@color/colorAccent"
        android:backgroundTint="#39b54a"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/frameLayout7"
        app:layout_constraintTop_toTopOf="parent">


    </FrameLayout>

    <RelativeLayout
        android:id="@+id/frameLayout7"
        android:layout_width="191dp"
        android:layout_height="178dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="3dp"
        android:background="@color/colorAccent"
        android:backgroundTint="#f15a24"
        app:layout_constraintEnd_toStartOf="@+id/frameLayout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="35dp"
            android:src="@drawable/rt" />


    </RelativeLayout>

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="191dp"
        android:layout_height="178dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="6dp"
        android:background="@color/colorAccent"
        android:backgroundTint="#f7931e"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/frameLayout4"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout">

    </FrameLayout>

    <TextView
        android:id="@+id/AirQualityApplication"
        android:layout_width="144dp"
        android:layout_height="16dp"
        android:layout_marginEnd="120dp"
        android:layout_marginBottom="1dp"
        android:text="@string/airquality"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="65dp"
        android:layout_height="18dp"
        android:layout_marginBottom="1dp"
        android:text="@string/beta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>